{{!
< default}} <div class="container">
    </div>
    <div class="container">
        <!-- if galleries -->
        {{#if galleries}} {{#each galleries}}
        <h2>{{name}}
		</h2>
        <div class="row gallery-images">
            <div class="col-sm-8 col-md-8 col-centered">
                <div class="row">
                    {{#each images}}
                    <div class="col-xs-4 col-sm-4 col-md-3 gallery-image ">
                        <img src="{{cloudinaryUrl width='300' height='300'}}" class="img-rounded">
                    </div>
                    {{/each}}
                </div>
            </div>
        </div>
        {{/each}} {{else}}
        <!-- else -->
        <h3 class="text-muted">There are no image galleries yet.</h3> {{/if}}
    </div>